                  <li class="filter-list"><input class="pixel-radio" type="radio" id="samsung" name="brand"><label for="samsung">Samsung<span>(19)</span></label></li>
                </ul>
              </form>
            </div>
            <div class="common-filter">
              <div class="head">Color</div>
              <form action="#">
                <ul>
                  <li class="filter-list"><input class="pixel-radio" type="radio" id="black" name="color"><label for="black">Black<span>(29)</span></label></li>
                  <li class="filter-list"><input class="pixel-radio" type="radio" id="balckleather" name="color"><label for="balckleather">Black
                      Leather<span>(29)</span></label></li>
                  <li class="filter-list"><input class="pixel-radio" type="radio" id="blackred" name="color"><label for="blackred">Black
                      with red<span>(19)</span></label></li>
                  <li class="filter-list"><input class="pixel-radio" type="radio" id="gold" name="color"><label for="gold">Gold<span>(19)</span></label></li>
                  <li class="filter-list"><input class="pixel-radio" type="radio" id="spacegrey" name="color"><label for="spacegrey">Spacegrey<span>(19)</span></label></li>
                </ul>
              </form>
            </div>
            
          </div>
        </div>
        <div class="col-xl-9 col-lg-8 col-md-7">
          <!-- Start Filter Bar -->
          <div class="filter-bar d-flex flex-wrap align-items-center">
            <div class="sorting">
              <select>
                <option value="1">Default sorting</option>
                <option value="1">Default sorting</option>
                <option value="1">Default sorting</option>
              </select>
            </div>
            <div class="sorting mr-auto">
              <select>
                <option value="1">Show 12</option>
                <option value="1">Show 12</option>
                <option value="1">Show 12</option>
              </select>
            </div>
            <div>
              <form action="/user/category/search" method="post">
	              <div class="input-group filter-bar-search">
	              
	                <input type="text"  name="keywords" placeholder="Search">
	                <div class="input-group-append">
	                  <button  type="submit" formaction="/user/category/search?field=name" ><i class="ti-search"></i></button>
	                </div>
					
	              </div>
              </form>
            </div>
          </div>
          <!-- End Filter Bar -->
          <!-- Start Best Seller -->
          <section class="lattest-product-area pb-40 category-list">
            <div class="row">
            
            
            <c:forEach var="item" items="${page.content}">
              
              
       
	              <div class="col-md-6 col-lg-4">
	                <div class="card text-center card-product">
	                  <div class="card-product__img">
	                    <img class="card-img" src="/templates/user/img/product/${item.image}" alt="">
	                    <ul class="card-product__imgOverlay">
	                      <li><button><i class="ti-search"></i></button></li>
	                      <li><a href="/user/cart/buy/${item.id }"> <button> <i class="ti-shopping-cart"></i></button></a></li>
	                      <li><a href="/user/cart/add/${item.id }"> <button><i class="ti-heart"></i></button></a></li>
	                    </ul>
	                  </div>
	                  <div class="card-body">
	                    <p>Accessories </p>
	                    <h4 class="card-product__title"><a href="/user/single-product/${item.id}">${item.name}</a></h4>
	                    <p class="card-product__price">$${item.price}</p>
	                    
	                  </div>
	                </div>
	              </div>
               </c:forEach>
               
                
            </div>
            <div>
           </div>
            <div style="text-align: center;">
              <a class="btn btn-secondary"style="width: 50px;" href="/user/category/brand?p=0"><<</a>
              <a class="btn btn-secondary" style="width: 50px;" href="/user/category/brand?p=${page.number-1}"><</a>
            <a class="btn btn-secondary" style="width: 50px;" href="/user/category/brand?p=${page.number+1}">></a>
            <a class="btn btn-secondary" style="width: 50px;" href="/user/category/brand?p=${page.totalPages-1}">>></a>
            </div>
           
          </section>
          <!-- End Best Seller -->
        </div>
      </div>
    </div>
  </section>

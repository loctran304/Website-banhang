      <div class="row">
        <div class="col-xl-3 col-lg-4 col-md-5">
          <div class="sidebar-categories">
            <div class="head">Browse Categories</div>
            <ul class="main-categories">
              <li class="common-filter">
                <form action="#">
                  <ul>
                    
                    <c:forEach var="itemc" items="${category}">
                   		<form action="/user/category/brand?field1=${itemc.id}" method="post">
                    		 <li class="filter-list"><input class="pixel-radio" type="radio" id="${itemc.name}" 
                    		  onchange="this.form.submit()" name="brand"><label for="${itemc.name}">${itemc.name}</label></li>
                    	</form>
                     </c:forEach>
                  </ul>
                </form>
              </li>
            </ul>
          </div>
          <div class="sidebar-filter">
          
            <div class="top-filter-head">Product Filters</div>
            <div class="common-filter">
              <div class="head ">Price</div>
	              <div class="price-range-area ">
                
	               <form action="/user/category" method="post">
						<div class="wrapper">
					      <div class="price-input">
					        <div class="field">
					          <span>Min</span>
					          <input type="number"  value="${min}"    class="min-input" />
					        </div>
					        <div class="seperator">-</div>
					        <div class="field">
					          <span>Max</span>
					          <input type="number"  value="${max}"   class="max-input" />
					        </div>
					      </div>
					      <div class="slider">
					        <div class="progress"></div>
					      </div>
					      <div class="range-input">
					
					        <input type="range" min="0" max="200"  value="0" name="min" class="min-range" />
					        <input type="range" min="0" max="200"  value="200" name="max"  class="max-range"/>
					        
					      </div>
					    </div>
					
						<button class="btn btn-secondary " style=" margin-top: 15px;  border-radius: 6px;">Search</button>
						<br>
						
				  </form>
                
              </div>
            </div>
            <div class="common-filter">
              <div class="head">Brands</div>
              <form action="#">
                <ul>
                  <li class="filter-list"><input class="pixel-radio" type="radio" id="apple" name="brand"><label for="apple">Apple<span>(29)</span></label></li>
                  <li class="filter-list"><input class="pixel-radio" type="radio" id="asus" name="brand"><label for="asus">Asus<span>(29)</span></label></li>
                  <li class="filter-list"><input class="pixel-radio" type="radio" id="gionee" name="brand"><label for="gionee">Gionee<span>(19)</span></label></li>
